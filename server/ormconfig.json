{
   "type": "sqlite",
   "database": "database.sqlite",
   "synchronize": false,
   "logging": false,
   "entities": [
      "server/src/entity/**/*.ts"
   ],
   "migrations": [
      "server/src/migration/**/*.ts"
   ],
   "subscribers": [
      "server/src/subscriber/**/*.ts"
   ],
   "cli": {
      "entitiesDir": "server/src/entity",
      "migrationsDir": "server/src/migration",
      "subscribersDir": "server/src/subscriber"
   }
}